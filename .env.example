API_KEY=

# Anthropic API Key
ANTHROPIC_API_KEY=sk-ant-...

# Claude model (claude-sonnet-4-5-20250929, claude-opus-4-6, claude-haiku-4-5-20251001)
CLAUDE_MODEL=claude-sonnet-4-5-20250929

# EVM RPC URLs
ETHEREUM_RPC_URL=https://eth.llamarpc.com
POLYGON_RPC_URL=https://polygon.llamarpc.com
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
OPTIMISM_RPC_URL=https://mainnet.optimism.io
BASE_RPC_URL=https://mainnet.base.org
AVALANCHE_RPC_URL=https://api.avax.network/ext/bc/C/rpc

# Solana RPC URLs
SOLANA_MAINNET_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_DEVNET_RPC_URL=https://api.devnet.solana.com

# MCP servers config file path
MCP_SERVERS_CONFIG_PATH=./mcp-servers.json

# Agent settings
MAX_TURNS=50
LOG_LEVEL=info

# mcp-swap settings
ZEROX_API_KEY=your-0x-api-key
JUPITER_API_KEY=

# mcp-hyperliquid settings
# Set to "true" to use Hyperliquid testnet instead of mainnet
HYPERLIQUID_TESTNET=

# Telegram bot settings
# Create a bot via @BotFather on Telegram to get a token
TELEGRAM_BOT_TOKEN=
# Restrict the Telegram bot to specific user IDs (optional, comma-separated, empty = allow all)
TELEGRAM_ALLOWED_USER_IDS=
# Encryption passphrase for per-user wallet private keys stored in SQLite
WALLET_ENCRYPTION_KEY=
# SQLite database path for Telegram bot user wallets
SQLITE_DB_PATH=./data/agent-wallet.db
